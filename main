    <script>

    function answer() {
        let trueorfalse = prompt("True or False?")
         trueorfalse = trueorfalse.trim(); //This gets rid of the whitespace before and after the word.
        trueorfalse = trueorfalse.toLowerCase();

        if (trueorfalse === "True" || trueorfalse === "true") {
            alert("That's correct!");
        } else {alert("Wrong! RG debuted at the 1984 Olympic Games in LA.")}}
       
      

        let torf = document.querySelector(".gymnastics");
        torf.addEventListener("click", answer);

