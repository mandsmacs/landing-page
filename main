    function buy () { 
        let name = prompt ("What is your name?");
       let h9 = document.querySelector("h9");

       h9.innerHTML = "Hi there, " + name + "!";

        alert ("Thank you " + name + ". Welcome to my page!");}

        let buyButton = document.querySelector(".macaronquestion");
    buyButton.addEventListener("click", buy);

